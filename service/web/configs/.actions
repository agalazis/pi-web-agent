{
    "pi-web-agent": {
        "version": "1.0",
        "actions": [
            {
                "title": "System Information",
                "identifier": "system-information",
                "command-group": [
                    [
                        {
                            "title": "Kernel",
                            "cmd": "uname -sr"
                        },
                        {
                            "title": "Host name",
                            "cmd": "uname -n"
                        },
                        {
                            "title": "CPU",
                            "cmd": " cat /proc/cpuinfo | grep \"Processor\" || { cat /proc/cpuinfo | grep \"model name\"; } | head -n 1 | cut -d : -f 2"
                        }
                    ],
                    [
                        {
                            "title": "Memory",
                            "cmd": "/usr/libexec/pi-web-agent/scripts/memory_information"
                        }
                    ],
                    [
                        {
                            "title": "Disk Information",
                            "format": "table",
                            "cmd": "df -hP /"
                        }
                    ]
                ]
            },
            {
                "title": "Processes",
                "identifier": "running-processes",
                "command-group": [
                    [
                        {
                            "title": "Running Processes",
                            "format": "table",
                            "cmd": "ps -e"
                        }
                    ]
                ]
            },
            {
                "title": "Scheduled Tasks",
                "identifier": "scheduled-tasks",
                "command-group": [
                    [
                        {
                            "title": "User",
                            "format": "table",
                            "cmd": "echo \"Command\"; crontab -l"
                        },
                        {
                            "title": "Hourly",
                            "format": "table",
                            "cmd": "echo \"Filename\"; ls /etc/cron.hourly"
                        }
                    ],
                    [
                        {
                            "title": "Daily",
                            "format": "table",
                            "cmd": "echo \"Filename\"; ls /etc/cron.daily"
                        }
                    ],
                    [
                        {
                            "title": "Weekly",
                            "format": "table",
                            "cmd": "echo \"Filename\"; ls /etc/cron.weekly"
                        }
                    ],
                    [
                        {
                            "title": "Monthly",
                            "format": "table",
                            "cmd": "echo \"Filename\"; ls /etc/cron.monthly"
                        }
                    ]
                ]
            }
        ]
    }
}
